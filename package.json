{
  "name": "ta-math",
  "version": "0.2.3",
  "description": "Technical analysis library in javascript. Calculates indicators and overlays from ohlcv data",
  "main": "ta-math.js",
  "scripts": {
    "build": "npm run build:node && npm run build:bro",
    "build:node": "rollup src/ta-math.mjs -f cjs > ta-math.js --silent && rollup test/test.mjs -f cjs > test/test.js --silent",
    "build:bro": "rollup src/ta-math.mjs -f iife --name=TA > dist/ta-math.browser.js --silent",
    "test": "npm run build && node test/test.js",
    "test:ui": "start ./examples/page.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/munrocket/ta-math.git"
  },
  "keywords": [
    "technical-analysis",
    "quantitative-finance",
    "indicators",
    "overlays",
    "finance",
    "library",
    "ohlcv"
  ],
  "author": "munrocket",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/munrocket/ta-math/issues"
  },
  "homepage": "https://github.com/munrocket/ta-math#readme",
  "dependencies": {},
  "devDependencies": {
    "rollup": "^0.63.4",
    "tap-spec": "^5.0.0",
    "tape": "^4.9.1"
  }
}
